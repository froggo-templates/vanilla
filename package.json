{
  "license": "UNLICENSED",
  "scripts": {
    "dev": "yarn clean && concurrently \"yarn watch:sass\" \"yarn watch:js\" \"yarn watch:11ty\"",
    "start": "yarn dev",
    "build": "yarn clean && concurrently \"yarn build:sass\" \"yarn build:js\" \"yarn build:11ty\"",
    "build:imt": "yarn build && node build-imt.js",
    "clean": "rimraf dist",
    "watch:sass": "sass --watch src/scss/main.scss:dist/assets/app.css",
    "build:sass": "sass --no-source-map src/scss/main.scss:dist/assets/app.css && yarn build:postcss",
    "build:postcss": "postcss dist/assets/app.css -o dist/assets/app.css",
    "watch:js": "rollup -c -w",
    "build:js": "BABEL_ENV=production rollup -c",
    "watch:11ty": "eleventy --serve --quiet --port=8888",
    "build:11ty": "eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "autoprefixer": "^10.4.2",
    "cheerio": "^1.0.0-rc.10",
    "concurrently": "^7.0.0",
    "core-js": "^3.21.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "postcss-minify": "^1.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sass": "^1.2.10",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.49.7"
  }
}
